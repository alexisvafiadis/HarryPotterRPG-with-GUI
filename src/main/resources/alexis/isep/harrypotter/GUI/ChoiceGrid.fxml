<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="alexis.isep.harrypotter.GUI.ChoiceGridController"
            prefHeight="130.0" prefWidth="347.0">
    <GridPane fx:id="gridPane" prefHeight="130.0" prefWidth="347.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
        <children>
            <Button fx:id="button1" mnemonicParsing="false" onAction="#handleActionButtonEvent" prefHeight="237.0" prefWidth="174.0" text="Look around">
                <font>
                    <Font name="Avenir Next Condensed Regular" size="24.0" />
                </font>
            </Button>
            <Button fx:id="button2" mnemonicParsing="false" onAction="#handleActionButtonEvent" prefHeight="237.0" prefWidth="174.0" text="Cast a spell" GridPane.columnIndex="1">
                <font>
                    <Font name="Avenir Next Condensed Regular" size="24.0" />
                </font>
            </Button>
            <Button fx:id="button3" mnemonicParsing="false" onAction="#handleActionButtonEvent" prefHeight="237.0" prefWidth="174.0" text="Hide" GridPane.rowIndex="1">
                <font>
                    <Font name="Avenir Next Condensed Regular" size="24.0" />
                </font>
            </Button>
            <Button fx:id="button4" mnemonicParsing="false" onAction="#handleActionButtonEvent" prefHeight="237.0" prefWidth="174.0" text="Use a potion" GridPane.columnIndex="1" GridPane.rowIndex="1">
                <font>
                    <Font name="Avenir Next Condensed Regular" size="24.0" />
                </font>
            </Button>
        </children>
    </GridPane>
</AnchorPane>
